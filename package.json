{
    "name": "mcpews-cf",
    "version": "4.0.0",
    "description": "A library that supports MCPE Websocket Protocol. customized by So Tokumaru for Cloudflare Workers",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "bin": {
        "mcpews": "./dist/repl.js",
        "mcpewsmitm": "./dist/mitm.js"
    },
    "files": [
        "dist/"
    ],
    "scripts": {
