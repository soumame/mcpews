        "ws": "^8.14.2"
    },
    "devDependencies": {
        "@types/jest": "^29.5.6",
        "@types/node": "^18.18.6",
        "@types/ws": "^8.5.8",
        "@typescript-eslint/eslint-plugin": "^6.9.0",
        "@typescript-eslint/parser": "^6.9.0",
        "eslint": "^8.52.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-jest": "^27.4.3",
        "eslint-plugin-prettier": "^5.0.1",
        "jest": "^29.7.0",
        "prettier": "^3.0.3",
        "rimraf": "^5.0.5",
        "ts-jest": "^29.1.1",
        "typedoc": "^0.25.2",
        "typescript": "^5.2.2"
